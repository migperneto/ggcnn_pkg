<launch>

    <group ns="objetos">

      <!-- add bico dosador -->
      <node name="bico_dosador" pkg="gazebo_ros" type="spawn_model" output="screen"
        args="-file $(find ggcnn_pkg)/models/bico_dosador/model.sdf -sdf -model bico_dosador -x -0.05 -y -0.5 -z 0.872 -R 0.0 -P 0.0 -Y 0.0" />

