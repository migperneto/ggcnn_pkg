      <node name="unob_3" pkg="gazebo_ros" type="spawn_model" output="screen"
        args="-file $(find ggcnn_pkg)/models/unob_3/model.sdf -sdf -model unob_3 -x -0.141 -y -0.45 -z 0.872 -R 0.0 -P 0.0 -Y 0.0" />

      <!-- add little_bin_box -->    
      <node name="little_bin_box" pkg="gazebo_ros" type="spawn_model" output="screen"
        args="-file $(find ggcnn_pkg)/models/little_bin_box/model.sdf -sdf -model little_bin_box -x 0.16 -y -0.45 -z 0.872 -R 0.0 -P 0.0 -Y 0.0" />

        <!-- add great_bin_box -->    
