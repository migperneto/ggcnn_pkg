      <!-- add unob_1 -->    
      <node name="unob_1" pkg="gazebo_ros" type="spawn_model" output="screen"
        args="-file $(find ggcnn_pkg)/models/unob_1/model.sdf -sdf -model unob_1 -x -0.058 -y -0.41 -z 0.872 -R 0.0 -P 0.0 -Y 0.0" />

